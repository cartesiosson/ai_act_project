@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ai: <http://ai-act.eu/ai#> .


<http://ai-act.eu/interop_intl>
    a owl:Ontology ;
    owl:versionIRI <http://ai-act.eu/interop_intl/0.35.0> ;
    owl:versionInfo "Versión 0.35.0 publicada el 2025-06-21"@es ,
                    "Version 0.35.0 released on 2025-06-21"@en ;
    rdfs:comment "Ontología del módulo interop_intl del AI Act."@es ,
                 "Interop_intl module of the AI Act ontology."@en ;
    dcterms:created "2025-06-21"^^xsd:date .

ai:InterfaceSpec a owl:Class ;
    rdfs:label "Interface Specification"@en,
        "Interface Specification"@es ;
    rdfs:comment "Especificación técnica de una interfaz utilizada por un sistema de inteligencia artificial para comunicarse con otros sistemas o usuarios."@en,
        "Especificación técnica de una interfaz utilizada por un sistema de inteligencia artificial para comunicarse con otros sistemas o usuarios."@es .

ai:hasInterface a ai:ObjectProperty ;
    rdfs:label "has interface"@en,
        "has interface"@es ;
    rdfs:comment "Relaciona un sistema de IA con la especificación de interfaz que implementa."@en,
        "Relaciona un sistema de IA con la especificación de interfaz que implementa."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range ai:InterfaceSpec .
