@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ai: <http://ai-act.eu/ai#> .


<http://ai-act.eu/governance_intl>
    a owl:Ontology ;
    owl:versionIRI <http://ai-act.eu/governance_intl/0.36.0> ;
    owl:versionInfo "Versión 0.36.0 publicada el 2025-06-22"@es ,
                    "Version 0.36.0 released on 2025-06-22"@en ;
    rdfs:comment "Ontología del módulo governance_intl del AI Act."@es ,
                 "Governance_intl module of the AI Act ontology."@en ;
    dcterms:created "2025-06-22"^^xsd:date .

ai:OversightBody a owl:Class ;
    rdfs:label "Oversight Body"@en,
        "Oversight Body"@es ;
    rdfs:comment "Entidad responsable de la supervisión y control de un sistema de inteligencia artificial, conforme a los mecanismos de gobernanza del AI Act."@en,
        "Entidad responsable de la supervisión y control de un sistema de inteligencia artificial, conforme a los mecanismos de gobernanza del AI Act."@es .

ai:isMonitoredBy a owl:ObjectProperty ;
    rdfs:label "is monitored by"@en,
        "is monitored by"@es ;
    rdfs:comment "Relaciona un sistema de IA con la entidad responsable de su supervisión."@en,
        "Relaciona un sistema de IA con la entidad responsable de su supervisión."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range ai:OversightBody .
