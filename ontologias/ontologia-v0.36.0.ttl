    rdfs:comment "Number of parameters in the AI model."@en,
        "Número de parámetros en el modelo de IA."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:long .

ai:hasComputationFLOPs a owl:DatatypeProperty ;
    rdfs:label "has computation FLOPs"@en, "tiene FLOPs de cómputo"@es ;
    rdfs:comment "Floating point operations used for training (FLOPs)."@en,
        "Operaciones de punto flotante utilizadas para entrenamiento (FLOPs)."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:decimal .

ai:hasAccuracyRate a owl:DatatypeProperty ;
    rdfs:label "has accuracy rate"@en, "tiene tasa de exactitud"@es ;
    rdfs:comment "Accuracy percentage of the AI system."@en,
        "Porcentaje de exactitud del sistema de IA."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:decimal .

ai:hasAutonomyLevel a owl:DatatypeProperty ;
    rdfs:label "has autonomy level"@en, "tiene nivel de autonomía"@es ;
    rdfs:comment "Level of autonomy (0-1 scale, where 1 is fully autonomous)."@en,
        "Nivel de autonomía (escala 0-1, donde 1 es completamente autónomo)."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:decimal .

ai:hasMarketReach a owl:DatatypeProperty ;
    rdfs:label "has market reach"@en, "tiene alcance de mercado"@es ;
    rdfs:comment "Number of registered business users in the Union."@en,
        "Número de usuarios comerciales registrados en la Unión."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:integer .

ai:isAdaptiveSystem a owl:DatatypeProperty ;
    rdfs:label "is adaptive system"@en, "es sistema adaptativo"@es ;
    rdfs:comment "Boolean indicating if system continues learning post-deployment."@en,
        "Booleano que indica si el sistema continúa aprendiendo post-despliegue."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:boolean .

