ai:TransformerModel a ai:AlgorithmType ;
    rdfs:label "Transformer Model"@en, "Modelo Transformer"@es ;
    rdfs:comment "Transformer-based architecture model."@en,
        "Modelo de arquitectura basada en Transformer."@es .

ai:FoundationModel a ai:AlgorithmType ;
    rdfs:label "Foundation Model"@en, "Modelo Fundacional"@es ;
    rdfs:comment "Large-scale pre-trained foundation model."@en,
        "Modelo fundacional pre-entrenado a gran escala."@es .

ai:GenerativeModel a ai:AlgorithmType ;
    rdfs:label "Generative Model"@en, "Modelo Generativo"@es ;
    rdfs:comment "Model capable of generating new content."@en,
        "Modelo capaz de generar nuevo contenido."@es .

ai:hasAlgorithmType a owl:ObjectProperty ;
    rdfs:label "has algorithm type"@en, "tiene tipo de algoritmo"@es ;
    rdfs:comment "Associates an AI system with its algorithm type."@en,
        "Asocia un sistema de IA con su tipo de algoritmo."@es ;
