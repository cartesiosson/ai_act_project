@prefix ai: <http://ai-act.eu/ai#> .
@prefix ctx: <http://ai-act.eu/context#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# ===== ADVANCED CONTEXTUAL CRITERIA - PHASE 3 =====
# Extended contextual criteria for complex AI system evaluation
# EU AI Act contextual analysis
# Version: 0.37.1 (2025-11-22)

# ===== VULNERABILITY DIMENSION CRITERIA =====

ctx:ChildrenAndMinorsVulnerabilityContext a ai:ContextualCriterion ;
    rdfs:label "Children and Minors Vulnerability"@en,
        "Vulnerabilidad de Niños y Menores"@es ;
    rdfs:comment "Specific protections required for AI systems interacting with or affecting children."@en,
        "Protecciones específicas requeridas para sistemas IA que interactúan con o afectan a niños."@es ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement ai:ParentalConsent ;
    ai:activatesRequirement ai:ProtectionOfMinors ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Child Safety Assessment"@en ;
        rdfs:comment "Evaluation of potential harms to children."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Age-Appropriate Design"@en ;
        rdfs:comment "Design must be appropriate for target age group."@en
    ] .

ctx:ElderlyAndDisabledVulnerabilityContext a ai:ContextualCriterion ;
    rdfs:label "Elderly and Disabled Vulnerability"@en,
        "Vulnerabilidad de Personas Mayores y Discapacitadas"@es ;
    rdfs:comment "Enhanced protections for systems affecting elderly or disabled populations."@en,
        "Protecciones mejoradas para sistemas que afectan a personas mayores o discapacitadas."@es ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Accessibility Compliance"@en ;
        rdfs:comment "System must be accessible to users with disabilities."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Cognitive Load Assessment"@en ;
        rdfs:comment "Assessment of cognitive demands on elderly users."@en
    ] .

ctx:SocioeconomicVulnerabilityContext a ai:ContextualCriterion ;
    rdfs:label "Socioeconomic Vulnerability"@en,
        "Vulnerabilidad Socioeconómica"@es ;
    rdfs:comment "Special considerations for systems affecting economically disadvantaged populations."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Equitable Access Assessment"@en ;
        rdfs:comment "Evaluation of system availability across socioeconomic groups."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Digital Divide Mitigation"@en ;
        rdfs:comment "Strategies to minimize digital divide impact."@en
    ] .

# ===== AUTONOMY AND CONTROL CRITERIA =====

ctx:AutonomousDecisionmakingContext a ai:ContextualCriterion ;
    rdfs:label "Autonomous Decision-making Context"@en,
        "Contexto de Toma de Decisiones Autónoma"@es ;
    rdfs:comment "Systems making decisions without human intervention require enhanced oversight."@en,
        "Sistemas que toman decisiones sin intervención humana requieren supervisión mejorada."@es ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement ai:HumanOversightRequirement ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Intervention Mechanism"@en ;
        rdfs:comment "Humans must be able to intervene in automated decisions."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Override Capability"@en ;
        rdfs:comment "System must allow human override of AI decisions."@en
    ] .

ctx:RealTimeAutonomousContext a ai:ContextualCriterion ;
    rdfs:label "Real-Time Autonomous Systems"@en,
        "Sistemas Autónomos en Tiempo Real"@es ;
    rdfs:comment "Systems operating in real-time with autonomous control require safety-critical design."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Real-Time Safety Guarantees"@en ;
        rdfs:comment "Formal safety assurances for real-time operation."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Latency Guarantees"@en ;
        rdfs:comment "Maximum acceptable response times defined and met."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Fallback Mechanisms"@en ;
        rdfs:comment "Defined fallback behavior if system fails."@en
    ] .

# ===== SYSTEMIC IMPACT CRITERIA =====

ctx:WidespreadSystemicImpactContext a ai:ContextualCriterion ;
    rdfs:label "Widespread Systemic Impact"@en,
        "Impacto Sistémico Generalizado"@es ;
    rdfs:comment "Systems with potential to impact many people or critical infrastructure."@en,
        "Sistemas con potencial de impactar a muchas personas o infraestructura crítica."@es ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Systemic Risk Assessment"@en ;
        rdfs:comment "Comprehensive assessment of systemic risks."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Cascading Failure Analysis"@en ;
        rdfs:comment "Analysis of how failures could cascade through systems."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Resilience Testing"@en ;
        rdfs:comment "Testing of system resilience under stress."@en
    ] .

ctx:CriticalInfrastructureInterdependencyContext a ai:ContextualCriterion ;
    rdfs:label "Critical Infrastructure Interdependency"@en,
        "Interdependencia de Infraestructura Crítica"@es ;
    rdfs:comment "Systems controlling or interfacing with critical infrastructure."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Interdependency Mapping"@en ;
        rdfs:comment "Detailed mapping of dependencies with other systems."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Contingency Planning"@en ;
        rdfs:comment "Plans for operation if dependent systems fail."@en
    ] .

# ===== ACCOUNTABILITY AND EXPLAINABILITY CRITERIA =====

ctx:BlackBoxDecisionContext a ai:ContextualCriterion ;
    rdfs:label "Black Box Decision Making"@en,
        "Toma de Decisiones de Caja Negra"@es ;
    rdfs:comment "Systems using unexplainable models for important decisions."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Explainability Requirement"@en ;
        rdfs:comment "Model decisions must be explainable to affected individuals."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Feature Importance Analysis"@en ;
        rdfs:comment "Explanation of which features influenced each decision."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Decision Appeal Mechanism"@en ;
        rdfs:comment "Users must be able to appeal AI decisions."@en
    ] .

ctx:HighStakesDecisionWithoutAppealContext a ai:ContextualCriterion ;
    rdfs:label "High-Stakes Decisions Without Appeal"@en,
        "Decisiones de Alto Impacto Sin Apelación"@es ;
    rdfs:comment "Irreversible high-impact decisions without appeal mechanisms."@en ;
    ai:assignsRiskLevel ai:UnacceptableRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Mandatory Appeal Mechanism"@en ;
        rdfs:comment "MUST provide mechanism to appeal or reverse decisions."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Human Review Authority"@en ;
        rdfs:comment "Trained humans must review appeals."@en
    ] .

# ===== FAIRNESS AND BIAS CRITERIA =====

ctx:HistoricalBiasReplicationContext a ai:ContextualCriterion ;
    rdfs:label "Historical Bias Replication Risk"@en,
        "Riesgo de Replicación de Sesgo Histórico"@es ;
    rdfs:comment "Systems trained on historical data that may contain discriminatory patterns."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Bias Audit"@en ;
        rdfs:comment "Comprehensive audit for discriminatory biases."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Bias Mitigation Strategy"@en ;
        rdfs:comment "Documented strategy to mitigate identified biases."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Fairness Metrics"@en ;
        rdfs:comment "Regular monitoring of fairness across demographic groups."@en
    ] .

ctx:ProtectedCharacteristicInferenceContext a ai:ContextualCriterion ;
    rdfs:label "Protected Characteristic Inference"@en,
        "Inferencia de Características Protegidas"@es ;
    rdfs:comment "Systems that can infer protected characteristics even if not explicitly input."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Proxy Variable Detection"@en ;
        rdfs:comment "Detection and mitigation of proxy variables for protected characteristics."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Inference Prevention"@en ;
        rdfs:comment "Technical measures to prevent protected characteristic inference."@en
    ] .

# ===== DATA PRIVACY AND SECURITY CRITERIA =====

ctx:BiometricDataProcessingContext a ai:ContextualCriterion ;
    rdfs:label "Biometric Data Processing"@en,
        "Procesamiento de Datos Biométricos"@es ;
    rdfs:comment "Systems processing biometric or highly sensitive personal data."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement ai:DataEncryption ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Biometric Storage Security"@en ;
        rdfs:comment "Enhanced security for biometric data storage."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Data Minimization"@en ;
        rdfs:comment "Collect only necessary biometric data."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Liveness Detection"@en ;
        rdfs:comment "For biometric identification, detect live presentation vs. spoofing."@en
    ] .

ctx:PersonalDataRetentionContext a ai:ContextualCriterion ;
    rdfs:label "Extended Personal Data Retention"@en,
        "Retención Extendida de Datos Personales"@es ;
    rdfs:comment "Systems retaining personal data beyond necessity."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Data Retention Policy"@en ;
        rdfs:comment "Clear policy on how long data is retained."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Automatic Deletion"@en ;
        rdfs:comment "Automatic deletion of data after retention period."@en
    ] .

# ===== ENVIRONMENTAL AND SOCIETAL IMPACT =====

ctx:LargeScaleEnvironmentalImpactContext a ai:ContextualCriterion ;
    rdfs:label "Large-Scale Environmental Impact"@en,
        "Impacto Ambiental a Gran Escala"@es ;
    rdfs:comment "AI systems with significant environmental impact (e.g., energy consumption in training)."@en ;
    ai:assignsRiskLevel ai:LimitedRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Environmental Impact Assessment"@en ;
        rdfs:comment "Assessment of energy and resource consumption."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Sustainability Measures"@en ;
        rdfs:comment "Measures to minimize environmental footprint."@en
    ] .

ctx:MisinformationAmplificationRiskContext a ai:ContextualCriterion ;
    rdfs:label "Misinformation Amplification Risk"@en,
        "Riesgo de Amplificación de Desinformación"@es ;
    rdfs:comment "Generative AI systems that could generate or amplify misinformation."@en ;
    ai:assignsRiskLevel ai:HighRisk ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Factuality Verification"@en ;
        rdfs:comment "Mechanisms to verify factual accuracy of generated content."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Source Attribution"@en ;
        rdfs:comment "Generated content must attribute sources when available."@en
    ] ;
    ai:activatesRequirement [
        a ai:ComplianceRequirement ;
        rdfs:label "Detection of Generated Content"@en ;
        rdfs:comment "Users should be able to detect AI-generated content."@en
    ] .

# ===== SUMMARY =====
# Total advanced contextual criteria: 15+
# Coverage areas:
# - Vulnerability dimensions (3 criteria)
# - Autonomy and control (3 criteria)
# - Systemic impact (2 criteria)
# - Accountability and explainability (3 criteria)
# - Fairness and bias (2 criteria)
# - Data privacy and security (2 criteria)
# - Environmental and societal impact (2 criteria)

# ===== END ADVANCED CONTEXTUAL CRITERIA =====
