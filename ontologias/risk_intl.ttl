@prefix ai1: <http://ai-act.eu/vocab#> .
@prefix comp: <http://ai-act.eu/ontology/compliance#> .
@prefix ex: <http://example.org/instances#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ai1:AutonomousWithoutExplanation a ai1:TechnicalCriterion,
        owl:NamedIndividual ;
    rdfs:label "Autonomous Without Explanation"@en,
        "Autonomous Without Explanation"@es .

ai1:BiometricIdentification ai1:activatesCriterion ai1:BiometricIdentificationCriterion,
        ai1:LacksHumanOversightCriterion,
        ai1:UsesBiometricIdentification ;
    ai1:expectedRiskLevel ai1:HighRisk .

ai1:Chatbot ai1:activatesCriterion ai1:LacksHumanOversightCriterion ;
    ai1:expectedRiskLevel ai1:LimitedRisk .

ai1:CreditScoring ai1:activatesCriterion ai1:CreditScoringUsageCriterion,
        ai1:EssentialServicesAccessCriterion,
        ai1:ImpactsFundamentalRightsCriterion ;
    ai1:expectedRiskLevel ai1:HighRisk .

ai1:CriticalInfrastructureCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Management of Critical Infrastructure"@en,
        "Management of Critical Infrastructure"@es ;
    rdfs:comment "El sistema se utiliza para la gestión y operación de infraestructuras críticas como energía, agua, transporte o telecomunicaciones."@en,
        "El sistema se utiliza para la gestión y operación de infraestructuras críticas como energía, agua, transporte o telecomunicaciones."@es .

ai1:DeepfakeGenerator ai1:expectedRiskLevel ai1:LimitedRisk .

ai1:EducationAccess ai1:activatesCriterion ai1:EducationEvaluationCriterion,
        ai1:ImpactsFundamentalRightsCriterion ;
    ai1:expectedRiskLevel ai1:HighRisk .

ai1:JudicialSupportCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Judicial and Democratic Processes Support"@en,
        "Judicial and Democratic Processes Support"@es ;
    rdfs:comment "El sistema asiste en la interpretación de hechos o derecho en procedimientos judiciales o administrativos."@en,
        "El sistema asiste en la interpretación de hechos o derecho en procedimientos judiciales o administrativos."@es .

ai1:LawEnforcementCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Law Enforcement and Risk Assessment"@en,
        "Law Enforcement and Risk Assessment"@es ;
    rdfs:comment "El sistema se utiliza en contextos policiales, evaluación de riesgo delictivo o análisis de pruebas."@en,
        "El sistema se utiliza en contextos policiales, evaluación de riesgo delictivo o análisis de pruebas."@es .

ai1:MigrationControl ai1:activatesCriterion ai1:ImpactsFundamentalRightsCriterion,
        ai1:MigrationBorderCriterion ;
    ai1:expectedRiskLevel ai1:HighRisk .

ai1:MinimalRisk a ai1:RiskLevel,
        owl:NamedIndividual ;
    rdfs:label "Minimal Risk"@en,
        "Minimal Risk"@es ;
    rdfs:comment "Sistemas de riesgo mínimo o nulo. No están sujetos a obligaciones específicas según el AI Act."@en,
        "Sistemas de riesgo mínimo o nulo. No están sujetos a obligaciones específicas según el AI Act."@es .

ai1:PublicServiceAllocation ai1:activatesCriterion ai1:EssentialServicesAccessCriterion,
        ai1:ImpactsFundamentalRightsCriterion ;
    ai1:expectedRiskLevel ai1:HighRisk .

ai1:RecruitmentEmploymentCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Recruitment and Employment Management"@en,
        "Recruitment and Employment Management"@es ;
    rdfs:comment "El sistema se utiliza para procesos de contratación, evaluación de desempeño o decisiones de empleo."@en,
        "El sistema se utiliza para procesos de contratación, evaluación de desempeño o decisiones de empleo."@es .

ai1:SocialScoring ai1:expectedRiskLevel ai1:UnacceptableRisk .

ai1:SubliminalManipulation ai1:activatesCriterion ai1:AutonomousWithoutExplanationCriterion ;
    ai1:expectedRiskLevel ai1:UnacceptableRisk .

ai1:UsesBiometricIdentificationCriterion ai1:triggersComplianceRequirement comp:AccuracyEvaluationRequirement,
        comp:DataGovernanceRequirement .

ai1:activatesCriterion a owl:ObjectProperty ;
    rdfs:label "activates criterion"@en,
        "activates criterion"@es ;
    rdfs:comment "El propósito funcional activa este criterio de riesgo normativo, técnico o contextual."@en,
        "El propósito funcional activa este criterio de riesgo normativo, técnico o contextual."@es ;
    rdfs:domain ai1:Purpose ;
    rdfs:range ai1:RiskCriterion .

ai1:expectedRiskLevel a owl:ObjectProperty ;
    rdfs:label "expected risk level"@en,
        "expected risk level"@es ;
    rdfs:comment "Nivel de riesgo esperado para un sistema que tiene este propósito, conforme al AI Act."@en,
        "Nivel de riesgo esperado para un sistema que tiene este propósito, conforme al AI Act."@es ;
    rdfs:domain ai1:Purpose ;
    rdfs:range ai1:RiskLevel .

ai1:hasRiskLevel a owl:ObjectProperty ;
    rdfs:label "has risk level"@en,
        "has risk level"@es ;
    rdfs:comment "Asocia un sistema de inteligencia artificial con su nivel de riesgo conforme al AI Act."@en,
        "Asocia un sistema de inteligencia artificial con su nivel de riesgo conforme al AI Act."@es ;
    rdfs:domain ai1:IntelligentSystem ;
    rdfs:range ai1:RiskLevel .

ai1:justifiedByCriterion a owl:ObjectProperty ;
    rdfs:label "justified by criterion"@en,
        "justified by criterion"@es ;
    rdfs:comment "Indica los criterios que justifican la asignación del nivel de riesgo al sistema."@en,
        "Indica los criterios que justifican la asignación del nivel de riesgo al sistema."@es ;
    rdfs:domain ai1:IntelligentSystem ;
    rdfs:range ai1:RiskCriterion .

ai1:triggersComplianceRequirement a owl:ObjectProperty ;
    rdfs:label "triggers compliance requirement"@en,
        "activa requisito de cumplimiento"@es ;
    rdfs:comment "Indica que un criterio de riesgo genera la necesidad de un requisito de cumplimiento específico."@en,
        "Indica que un criterio de riesgo genera la necesidad de un requisito de cumplimiento específico."@es ;
    rdfs:domain ai1:RiskCriterion ;
    rdfs:range comp:ComplianceRequirement .

ex:System_A a ai1:IntelligentSystem ;
    ai1:hasRiskLevel ai1:HighRisk ;
    ai1:justifiedByCriterion ai1:BiometricIdentificationCriterion,
        ai1:ImpactsFundamentalRights,
        ai1:LacksHumanOversight .

ai1:AutonomousWithoutExplanationCriterion ai1:triggersComplianceRequirement comp:TransparencyRequirement .

ai1:ContextualCriterion a owl:Class ;
    rdfs:label "Contextual Criterion"@en,
        "Contextual Criterion"@es ;
    rdfs:comment "Criterio basado en el entorno, propósito o consecuencias del uso del sistema."@en,
        "Criterio basado en el entorno, propósito o consecuencias del uso del sistema."@es ;
    rdfs:subClassOf ai1:RiskCriterion .

ai1:EducationEvaluationCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Education or Vocational Training Evaluation"@en,
        "Education or Vocational Training Evaluation"@es ;
    ai1:triggersComplianceRequirement comp:DataGovernanceRequirement ;
    rdfs:comment "El sistema se utiliza para evaluar estudiantes o decidir su acceso a instituciones educativas o formativas."@en,
        "El sistema se utiliza para evaluar estudiantes o decidir su acceso a instituciones educativas o formativas."@es .

ai1:ImpactsFundamentalRights a ai1:ContextualCriterion,
        owl:NamedIndividual ;
    rdfs:label "Impacts Fundamental Rights"@en,
        "Impacts Fundamental Rights"@es .

ai1:LacksHumanOversight a ai1:TechnicalCriterion,
        owl:NamedIndividual ;
    rdfs:label "Lacks Human Oversight"@en,
        "Lacks Human Oversight"@es .

ai1:MigrationBorderCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Migration, Asylum and Border Control"@en,
        "Migration, Asylum and Border Control"@es ;
    rdfs:comment "El sistema se utiliza en la gestión de migración, control fronterizo o verificación de documentos."@en,
        "El sistema se utiliza en la gestión de migración, control fronterizo o verificación de documentos."@es .

ai1:UsesBiometricIdentification a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Uses Biometric Identification"@en,
        "Uses Biometric Identification"@es .

ai1:BiometricIdentificationCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Biometric Identification or Categorization"@en,
        "Biometric Identification or Categorization"@es ;
    rdfs:comment "El sistema realiza identificación biométrica remota o categorización de personas, conforme al punto 1 del Anexo III del AI Act."@en,
        "El sistema realiza identificación biométrica remota o categorización de personas, conforme al punto 1 del Anexo III del AI Act."@es .

ai1:EssentialServicesAccessCriterion a ai1:NormativeCriterion,
        owl:NamedIndividual ;
    rdfs:label "Access to Essential Services"@en,
        "Access to Essential Services"@es ;
    ai1:triggersComplianceRequirement comp:TraceabilityRequirement ;
    rdfs:comment "El sistema se utiliza para determinar el acceso de personas a servicios esenciales como vivienda, sanidad, crédito o educación."@en,
        "El sistema se utiliza para determinar el acceso de personas a servicios esenciales como vivienda, sanidad, crédito o educación."@es .

ai1:LacksHumanOversightCriterion ai1:triggersComplianceRequirement comp:HumanOversightRequirement .

ai1:LimitedRisk a ai1:RiskLevel,
        owl:NamedIndividual ;
    rdfs:label "Limited Risk"@en,
        "Limited Risk"@es ;
    rdfs:comment "Sistemas que requieren obligaciones de transparencia. Por ejemplo, chatbots o generadores de contenido."@en,
        "Sistemas que requieren obligaciones de transparencia. Por ejemplo, chatbots o generadores de contenido."@es .

ai1:TechnicalCriterion a owl:Class ;
    rdfs:label "Technical Criterion"@en,
        "Technical Criterion"@es ;
    rdfs:comment "Criterio basado en la estructura, funcionamiento o diseño técnico del sistema."@en,
        "Criterio basado en la estructura, funcionamiento o diseño técnico del sistema."@es ;
    rdfs:subClassOf ai1:RiskCriterion .

ai1:UnacceptableRisk a ai1:RiskLevel,
        owl:NamedIndividual ;
    rdfs:label "Unacceptable Risk"@en,
        "Unacceptable Risk"@es ;
    rdfs:comment "Sistemas que están prohibidos en la UE por representar una amenaza clara para la seguridad o los derechos."@en,
        "Sistemas que están prohibidos en la UE por representar una amenaza clara para la seguridad o los derechos."@es .

ai1:ImpactsFundamentalRightsCriterion ai1:triggersComplianceRequirement comp:FundamentalRightsAssessmentRequirement .

ai1:HighRisk a ai1:RiskLevel,
        owl:NamedIndividual ;
    rdfs:label "High Risk"@en,
        "High Risk"@es ;
    rdfs:comment "Sistemas con alto impacto en derechos fundamentales, sujetos a requisitos estrictos de cumplimiento y gobernanza."@en,
        "Sistemas con alto impacto en derechos fundamentales, sujetos a requisitos estrictos de cumplimiento y gobernanza."@es .

ai1:RiskCriterion a owl:Class ;
    rdfs:label "Risk Criterion"@en,
        "Risk Criterion"@es ;
    rdfs:comment "Criterio normativo, técnico o contextual utilizado para clasificar el riesgo de un sistema de inteligencia artificial."@en,
        "Criterio normativo, técnico o contextual utilizado para clasificar el riesgo de un sistema de inteligencia artificial."@es .

ai1:RiskLevel a owl:Class ;
    rdfs:label "Risk Level"@en,
        "Risk Level"@es ;
    rdfs:comment "Nivel de riesgo de un sistema de inteligencia artificial según la clasificación establecida por el Reglamento Europeo de IA."@en,
        "Nivel de riesgo de un sistema de inteligencia artificial según la clasificación establecida por el Reglamento Europeo de IA."@es .

ai1:NormativeCriterion a owl:Class ;
    rdfs:label "Normative Criterion"@en,
        "Normative Criterion"@es ;
    rdfs:comment "Criterio derivado directamente del texto legal del AI Act."@en,
        "Criterio derivado directamente del texto legal del AI Act."@es ;
    rdfs:subClassOf ai1:RiskCriterion .

