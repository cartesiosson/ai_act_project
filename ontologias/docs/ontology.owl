    <!-- http://ai-act.eu/ai#CriticalInfrastructureOperation -->


    <owl:NamedIndividual rdf:about="http://ai-act.eu/ai#CriticalInfrastructureOperation">
        <rdf:type rdf:resource="http://ai-act.eu/ai#Purpose"/>
        <ai:activatesCriterion rdf:resource="http://ai-act.eu/ai#CriticalInfrastructureCriterion"/>
        <ai:triggersCriterion rdf:resource="http://ai-act.eu/ai#CriticalInfrastructureCriterion"/>
        <ai:expectedRiskLevel rdf:resource="http://ai-act.eu/ai#HighRisk"/>
