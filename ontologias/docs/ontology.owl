        <ai:assignsRiskLevel rdf:resource="http://ai-act.eu/ai#HighRisk"/>
        <rdfs:comment xml:lang="es">Sistema que puede generar planes y estrategias para lograr objetivos especificados.</rdfs:comment>
        <rdfs:comment xml:lang="en">System can generate plans and strategies to achieve specified objectives.</rdfs:comment>
        <rdfs:label xml:lang="es">Capacidad de Planificación</rdfs:label>
        <rdfs:label xml:lang="en">Planning Capability</rdfs:label>
    </owl:NamedIndividual>
    


