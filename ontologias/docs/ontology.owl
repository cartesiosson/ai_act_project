    </owl:Class>
    


    <!-- http://ai-act.eu/ai#FundamentalRightsImpactAssessment -->


    <owl:Class rdf:about="http://ai-act.eu/ai#FundamentalRightsImpactAssessment">
        <rdfs:subClassOf rdf:resource="http://ai-act.eu/ai#ComplianceRequirement"/>
        <rdfs:comment xml:lang="en">Art. 27 requirement: deployers of high-risk AI systems shall perform FRIA before putting system into use, particularly when vulnerable groups are affected.</rdfs:comment>
        <rdfs:label xml:lang="es">Evaluación de Impacto en Derechos Fundamentales</rdfs:label>
        <rdfs:label xml:lang="en">Fundamental Rights Impact Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://ai-act.eu/ai#FundamentalRightsInfringement -->


    <owl:Class rdf:about="http://ai-act.eu/ai#FundamentalRightsInfringement">
        <rdfs:subClassOf rdf:resource="http://ai-act.eu/ai#SeriousIncident"/>
    </owl:Class>
    


    <!-- http://ai-act.eu/ai#GeneralPurposeAIModel -->


    <owl:Class rdf:about="http://ai-act.eu/ai#GeneralPurposeAIModel">
        <rdfs:subClassOf rdf:resource="http://ai-act.eu/ai#IntelligentSystem"/>
        <rdfs:comment xml:lang="en">[GPAI] AI model that exhibits similar capabilities to other models and can be used for a wide range of downstream applications. Defined in Article 3(1d) of EU AI Act.</rdfs:comment>
        <rdfs:comment xml:lang="es">[GPAI] Modelo de IA que exhibe capacidades similares a otros modelos y puede usarse para una amplia gama de aplicaciones descendentes. Definido en el Artículo 3(1d) del AI Act.</rdfs:comment>
        <rdfs:label xml:lang="en">General Purpose AI Model</rdfs:label>
        <rdfs:label xml:lang="es">Modelo de IA de Propósito General</rdfs:label>
    </owl:Class>
    


    <!-- http://ai-act.eu/ai#HighCapabilityGPAIModel -->


    <owl:Class rdf:about="http://ai-act.eu/ai#HighCapabilityGPAIModel">
        <rdfs:subClassOf rdf:resource="http://ai-act.eu/ai#GeneralPurposeAIModel"/>
        <rdfs:comment xml:lang="en">[GPAI] GPAI models with systemic risk due to their high level of capability, particularly in areas like dual-use potential and broad applicability.</rdfs:comment>
        <rdfs:comment xml:lang="es">[GPAI] Modelos GPAI con riesgo sistémico debido a su alto nivel de capacidad.</rdfs:comment>
        <rdfs:label xml:lang="en">High Capability GPAI Model</rdfs:label>
        <rdfs:label xml:lang="es">Modelo GPAI de Alta Capacidad</rdfs:label>
    </owl:Class>
    


