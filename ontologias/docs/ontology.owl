    <!-- http://ai-act.eu/ai#RecruitmentEmploymentCriterion -->


    <owl:NamedIndividual rdf:about="http://ai-act.eu/ai#RecruitmentEmploymentCriterion">
        <rdf:type rdf:resource="http://ai-act.eu/ai#NormativeCriterion"/>
        <ai:activatesRequirement rdf:resource="http://ai-act.eu/ai#AccuracyEvaluationRequirement"/>
        <ai:activatesRequirement rdf:resource="http://ai-act.eu/ai#BiasDetectionRequirement"/>
        <ai:activatesRequirement rdf:resource="http://ai-act.eu/ai#DataGovernanceRequirement"/>
        <ai:activatesRequirement rdf:resource="http://ai-act.eu/ai#DocumentationRequirement"/>
