# ===== CORE PROPERTIES - UNCHANGED FROM v0.37.1 =====

ai:activatesCriterion a owl:ObjectProperty ;
    rdfs:label "activates Criterion"@en ;
    rdfs:comment "Indicates that a Purpose directly activates a Criterion."@en ;
    rdfs:domain ai:Purpose ;
    rdfs:range ai:Criterion .

ai:triggersCriterion a owl:ObjectProperty ;
    rdfs:label "triggers Criterion"@en ;
    rdfs:comment "Indicates that a DeploymentContext triggers a Criterion."@en ;
    rdfs:domain ai:DeploymentContext ;
    rdfs:range ai:Criterion .

ai:assignsRiskLevel a owl:ObjectProperty ;
    rdfs:label "assigns Risk Level"@en ;
    rdfs:comment "Indicates the risk level assigned by a Criterion."@en ;
    rdfs:domain ai:Criterion ;
    rdfs:range ai:RiskLevel .

ai:activatesRequirement a owl:ObjectProperty ;
    rdfs:label "activates Requirement"@en ;
    rdfs:comment "Indicates that a Criterion activates a Requirement."@en ;
    rdfs:domain ai:Criterion ;
    rdfs:range ai:ComplianceRequirement .

ai:hasSubRequirement a owl:ObjectProperty ;
    rdfs:label "has Sub-Requirement"@en ;
    rdfs:comment "Indicates sub-requirements nested within a parent Requirement."@en ;
    rdfs:domain ai:ComplianceRequirement ;
    rdfs:range ai:ComplianceRequirement .

ai:hasName a owl:DatatypeProperty ;
    rdfs:label "has Name"@en ;
    rdfs:comment "Name or identifier for an entity."@en ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:hasVersion a owl:DatatypeProperty ;
    rdfs:label "has Version"@en ;
    rdfs:comment "Version identifier for an entity."@en ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:hasDocumentationType a owl:DatatypeProperty ;
    rdfs:label "has Documentation Type"@en ;
    rdfs:comment "Type of documentation required."@en ;
    rdfs:domain ai:DocumentationRequirement ;
    rdfs:range xsd:string .

ai:mandatoryCompliance a owl:DatatypeProperty ;
    rdfs:label "mandatory Compliance"@en ;
    rdfs:comment "Boolean indicating if compliance is mandatory."@en ;
    rdfs:domain ai:ComplianceRequirement ;
    rdfs:range xsd:boolean .

