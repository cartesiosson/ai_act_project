@prefix ai: <http://ai-act.eu/ai#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# =============================================================
# REGLAS SWRL BÃSICAS - CONTEXTUALES Y NORMATIVAS (1-12)
# =============================================================

# REGLA 1: Education Context/Purpose -> ProtectionOfMinors
[ rdf:type swrl:Imp ;
  rdfs:label "Education Context triggers Protection of Minors" ;
  rdfs:comment "Systems in educational context or with educational purpose must protect minors" ;
  swrl:body ( [ rdf:type swrl:ClassAtom ;
               swrl:classPredicate ai:IntelligentSystem ;
               swrl:argument1 ?system ]
             [ rdf:type swrl:DatavaluedPropertyAtom ;
               swrl:propertyPredicate ai:hasDeploymentContext ;
               swrl:argument1 ?system ;
               swrl:argument2 ai:Education ]
           ) ;
  swrl:head ( [ rdf:type swrl:IndividualPropertyAtom ;
               swrl:propertyPredicate ai:hasActivatedCriterion ;
               swrl:argument1 ?system ;
               swrl:argument2 ai:ProtectionOfMinors ] ) ] .

[ rdf:type swrl:Imp ;
  rdfs:label "Education Purpose triggers Protection of Minors" ;
  swrl:body ( [ rdf:type swrl:ClassAtom ;
               swrl:classPredicate ai:IntelligentSystem ;
               swrl:argument1 ?system ]
             [ rdf:type swrl:DatavaluedPropertyAtom ;
               swrl:propertyPredicate ai:hasPurpose ;
               swrl:argument1 ?system ;
               swrl:argument2 ai:EducationAccess ]
           ) ;
  swrl:head ( [ rdf:type swrl:IndividualPropertyAtom ;
               swrl:propertyPredicate ai:hasActivatedCriterion ;
               swrl:argument1 ?system ;
               swrl:argument2 ai:ProtectionOfMinors ] ) ] .

