ai:IntelligentSystem a owl:Class ;
    rdfs:label "Intelligent System"@en,
        "Sistema Inteligente"@es ;
    rdfs:comment "AI-based system that may be subject to provision, deployment, use, import or distribution in the market."@en,
        "Sistema basado en inteligencia artificial que puede ser objeto de provisión, despliegue, uso, importación o distribución en el mercado."@es ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:anyURI ;
            owl:onProperty ai:hasHttpIri ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:anyURI ;
            owl:onProperty ai:hasUrn ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty ai:hasHttpIri ],
        ai:Identifiable .

[] a sh:NodeShape ;
    sh:property [ sh:datatype xsd:string ;
            sh:message "Value must be a valid UUID URN (urn:uuid:...)" ;
            sh:path ai:hasUrn ;
            sh:pattern "^urn:uuid:[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$" ] ;
    sh:targetSubjectsOf ai:hasUrn .



