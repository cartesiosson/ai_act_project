@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ai: <http://ai-act.eu/vocab#> .
@prefix comp: <http://ai-act.eu/ontology/compliance#> .














comp:AccuracyEvaluationRequirement a comp:ComplianceRequirement,
        owl:NamedIndividual ;
    rdfs:label "Accuracy Evaluation Requirement"@en,
        "Requisito de evaluación de precisión"@es .

comp:DataGovernanceRequirement a comp:ComplianceRequirement,
        owl:NamedIndividual ;
    rdfs:label "Data Governance Requirement"@en,
        "Gobernanza de datos"@es .

comp:FundamentalRightsAssessmentRequirement a comp:ComplianceRequirement,
        owl:NamedIndividual ;
    rdfs:label "Fundamental Rights Assessment Requirement"@en,
        "Evaluación de derechos fundamentales"@es .

comp:HumanOversightRequirement a comp:ComplianceRequirement,
        owl:NamedIndividual ;
    rdfs:label "Human Oversight Requirement"@en,
        "Supervisión humana"@es .

comp:TraceabilityRequirement a comp:ComplianceRequirement,
        owl:NamedIndividual ;
    rdfs:label "Traceability Requirement"@en,
        "Trazabilidad"@es .

comp:hasRequirement a owl:ObjectProperty ;
    rdfs:label "has compliance requirement"@en,
        "has compliance requirement"@es ;
    rdfs:comment "Asocia un sistema de IA con los requisitos de cumplimiento que le son aplicables."@en,
        "Asocia un sistema de IA con los requisitos de cumplimiento que le son aplicables."@es ;
    rdfs:domain <http://ai-act.eu/ontology#IntelligentSystem> ;
    rdfs:range comp:ComplianceRequirement .

ai:requiresCompliance a owl:ObjectProperty ;
    rdfs:label "requires compliance"@en,
        "requiere cumplimiento"@es ;
    rdfs:comment "Compliance requirements arising from the system's risk assessment."@en,
        "Requisitos de cumplimiento derivados de la evaluación de riesgo del sistema."@es ;
    rdfs:domain ai:RiskAssessment ;
    rdfs:range comp:ComplianceRequirement .

comp:ComplianceRequirement a owl:Class ;
    rdfs:label "Compliance Requirement"@en,
        "Compliance Requirement"@es ;
    rdfs:comment "Requisito normativo o técnico que un sistema de inteligencia artificial debe cumplir según el AI Act."@en,
        "Requisito normativo o técnico que un sistema de inteligencia artificial debe cumplir según el AI Act."@es .
