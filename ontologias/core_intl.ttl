@prefix ai: <http://ai-act.eu/vocab#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ai:hasDeploymentContext a owl:DatatypeProperty ;
    rdfs:comment "Entorno o dominio en el que se despliega el sistema (educación, salud, transporte, etc.)."@en,
        "Entorno o dominio en el que se despliega el sistema (educación, salud, transporte, etc.)."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:hasName a owl:DatatypeProperty ;
    rdfs:comment "Nombre del sistema de inteligencia artificial."@en,
        "Nombre del sistema de inteligencia artificial."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:hasPurpose a owl:DatatypeProperty ;
    rdfs:comment "Propósito previsto para el sistema de IA (por ejemplo, identificación biométrica, scoring crediticio, etc.)."@en,
        "Propósito previsto para el sistema de IA (por ejemplo, identificación biométrica, scoring crediticio, etc.)."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:hasRiskLevel a owl:DatatypeProperty ;
    rdfs:comment "Nivel de riesgo del sistema según la clasificación del AI Act (e.g., prohibited, high-risk, limited-risk, minimal-risk)."@en,
        "Nivel de riesgo del sistema según la clasificación del AI Act (e.g., prohibited, high-risk, limited-risk, minimal-risk)."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:hasTrainingDataOrigin a owl:DatatypeProperty ;
    rdfs:comment "Origen de los datos de entrenamiento utilizados por el sistema (internos, públicos, sintéticos, etc.)."@en,
        "Origen de los datos de entrenamiento utilizados por el sistema (internos, públicos, sintéticos, etc.)."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:hasVersion a owl:DatatypeProperty ;
    rdfs:comment "Versión o identificación técnica del sistema."@en,
        "Versión o identificación técnica del sistema."@es ;
    rdfs:domain ai:IntelligentSystem ;
    rdfs:range xsd:string .

ai:IntelligentSystem a owl:Class ;
    rdfs:comment "Sistema basado en inteligencia artificial que puede ser objeto de provisión, despliegue, uso, importación o distribución en el mercado."@en,
        "Sistema basado en inteligencia artificial que puede ser objeto de provisión, despliegue, uso, importación o distribución en el mercado."@es .

